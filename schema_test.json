  /api/v1/reports/financial-summary/:
    get:
      operationId: api_v1_reports_financial_summary_retrieve
      description: "\n    \U0001F4CA Financial Summary Report API\n    \n    Returns\
        \ comprehensive financial overview including:\n    - Total receivable and\
        \ payable amounts\n    - Settled vs unsettled breakdowns\n    - Net balance\
        \ calculation\n    - By counterparty (organization) breakdown\n    - By agent\
        \ breakdown\n    \n    **Calculation Rules:**\n    - Receivable: SUM(amount)\
        \ from Ledger where to_company_id = organization_id\n    - Payable: SUM(amount)\
        \ from Ledger where from_company_id = organization_id\n    - Net Balance:\
        \ total_receivable_amount - total_payable_amount\n    - Settled/Unsettled\
        \ split based on status field\n    "
      summary: Financial / Ledger Summary Report
      parameters:
      - in: query
        name: organization_id
        schema:
          type: integer
        description: Organization ID (required)
        required: true
      tags:
      - api
      security:
      - jwtAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                  data:
                    type: object
                    properties:
                      organization_id:
                        type: integer
                      total_receivable_amount:
                        type: number
                      total_payable_amount:
                        type: number
                      receivable_settled_amount:
                        type: number
                      receivable_unsettled_amount:
                        type: number
                      payable_settled_amount:
                        type: number
                      payable_unsettled_amount:
                        type: number
                      net_balance:
                        type: number
                      by_counterparty:
                        type: array
                      by_agent:
                        type: array
          description: ''
  /api/v1/reports/sales-summary/:
    get:
      operationId: api_v1_reports_sales_summary_retrieve
      description: "\n    \U0001F4CA Sales Summary Report API\n    \n    Returns comprehensive\
        \ sales statistics including:\n    - Total bookings by category (ticket, umrah,\
        \ visa, hotel, transport, food, ziyarat)\n    - Payment status breakdown (paid,\
        \ unpaid, expired)\n    - Amount summaries\n    - Agent-wise breakdown with\
        \ service details\n    \n    **Calculation Rules:**\n    - From Booking table:\
        \ COUNT(all bookings), SUM(total_amount)\n    - total_paid_amount = SUM(total_amount\
        \ WHERE payment_status = \"Paid\")\n    - total_hotel_nights = SUM of BookingHotelDetails.total_nights\n\
        \    - Filters applied on booking_date/created_at\n    "
      summary: Sales Counting Report
      parameters:
      - in: query
        name: agent_id
        schema:
          type: integer
        description: Filter by specific agent (optional)
      - in: query
        name: branch_id
        schema:
          type: integer
        description: Filter by specific branch (optional)
      - in: query
        name: date_from
        schema:
          type: string
          format: date
        description: 'Start date (YYYY-MM-DD). Default: first day of current month'
      - in: query
        name: date_to
        schema:
          type: string
          format: date
        description: 'End date (YYYY-MM-DD). Default: last day of current month'
      - in: query
        name: organization_id
        schema:
          type: integer
        description: Organization ID (required)
        required: true
      tags:
      - api
      security:
      - jwtAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                  data:
                    type: object
                    properties:
                      total_bookings:
                        type: integer
                      total_group_bookings:
                        type: integer
                      total_ticket_bookings:
                        type: integer
                      total_umrah_bookings:
                        type: integer
                      total_visa_bookings:
                        type: integer
                      total_hotel_nights:
                        type: integer
                      total_transport_bookings:
                        type: integer
                      total_food_bookings:
                        type: integer
                      total_ziyarat_bookings:
                        type: integer
                      total_paid_orders:
                        type: integer
                      total_unpaid_orders:
                        type: integer
                      total_expired_orders:
                        type: integer
                      total_amount:
                        type: number
                      total_paid_amount:
                        type: number
                      total_unpaid_amount:
                        type: number
                      total_expired_amount:
                        type: number
                      agent_wise_summary:
                        type: array
          description: ''
  /api/v1/reports/top-sellers/:
    get:
      operationId: api_v1_reports_top_sellers_retrieve
      description: "\n    \U0001F4CA Top Seller Report API\n    \n    Returns ranking\
        \ of agents by total sales or booking count with:\n    - Agent-wise sales\
        \ totals\n    - Category breakdown per agent\n    - Ranking based on total_amount\
        \ or total_bookings\n    \n    **Calculation Logic:**\n    - Group bookings\
        \ by agent_id\n    - Count total bookings per agent\n    - Sum total_amount\
        \ per agent\n    - Group by (agent_id, category) for breakdown\n    - Sort\
        \ by total_amount or total_bookings descending\n    - Assign sequential ranking\n\
        \    "
      summary: Top Seller (Agent-Wise) Report
      parameters:
      - in: query
        name: date_from
        schema:
          type: string
          format: date
        description: Start date (YYYY-MM-DD). Optional
      - in: query
        name: date_to
        schema:
          type: string
          format: date
        description: End date (YYYY-MM-DD). Optional
      - in: query
        name: limit
        schema:
          type: integer
        description: 'Number of top sellers to return (default: 10)'
      - in: query
        name: organization_id
        schema:
          type: integer
        description: Organization ID (required)
        required: true
      - in: query
        name: sort_by
        schema:
          type: string
          enum:
          - total_amount
          - total_bookings
        description: 'Sort by ''total_amount'' or ''total_bookings'' (default: total_amount)'
      tags:
      - api
      security:
      - jwtAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                  data:
                    type: array
                    items:
                      type: object
                      properties:
                        agent_id:
                          type: integer
                        agent_name:
                          type: string
                        total_bookings:
                          type: integer
                        total_amount:
                          type: number
                        categories:
                          type: array
                        ranking:
                          type: integer
          description: ''
