          type: string
          readOnly: true
        pax:
          type: integer
          readOnly: true
        pax_id_str:
          type: string
          readOnly: true
        pax_first_name:
          type: string
          readOnly: true
        pax_last_name:
          type: string
          readOnly: true
        pax_full_name:
          type: string
          readOnly: true
        booking_id_str:
