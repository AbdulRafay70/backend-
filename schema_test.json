        required: true
      tags:
      - api
      security:
      - jwtAuth: []
      - {}
      responses:
        '200':
          content:
            application/json:
              schema:
                description: List of area agents with pending balances
                example:
                  organization_id: '5'
                  total_pending_area_agents: 2
                  area_agents:
                  - area_agent_id: 1
                    area_agent_name: John Doe
                    contact_no: '1234567890'
                    pending_balance: -5000.0
                    payment_promise_ids:
                    - 1
                    - 2
                    - 3
                    branch_id: B001
          description: ''
        '400':
          content:
            application/json:
