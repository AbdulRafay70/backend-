          type: integer
        description: Filter by branch ID
      - in: query
        name: is_standalone
        schema:
          type: boolean
        description: Show only standalone forms
      - in: query
        name: linked_blog_id
        schema:
          type: integer
        description: Filter by linked blog ID
      - in: query
        name: organization_id
        schema:
          type: integer
        description: Filter by organization ID
      - in: query
        name: status
        schema:
          type: string
        description: Filter by status
